<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageId>Genesis.Package.Template</PackageId>
		<Authors>Mohmad Sufiyan</Authors>
		<Description>Base template to create NuGet packages</Description>
		<RepositoryUrl>https://github.com/Dev-Sufiyan/Genesis.Package.Template</RepositoryUrl>
	</PropertyGroup>
	<ItemGroup>
	  <PackageReference Include="Genesis.Repositories" Version="1.0.0" />
	</ItemGroup>
</Project>
